	<f:entry title="${%label.target.directory}" field="target">
		<f:textbox />
	</f:entry>
        
        <f:optionalBlock title="${%label.additional.options}" field="hasOptions" inline="true">

            <f:entry field="openSourceOnly">
		<f:checkbox title="${%label.opensource.only}" onclick="openSourceOnlyClicked(this);" name="openSourceOnly"/>
            </f:entry>

            <f:entry field="sourceCodeOnly">
        <f:checkbox title="${%label.sourcecode.only}" onclick="sourceCodeOnlyClicked(this);" name="sourceCodeOnly"/>
			</f:entry>
	</f:optionalBlock>

</j:jelly>
