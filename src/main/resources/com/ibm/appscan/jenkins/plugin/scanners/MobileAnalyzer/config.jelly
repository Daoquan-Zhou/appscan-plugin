	<f:entry title="${%Application File}" field="target">
		<f:textbox />
	</f:entry>

	<f:optionalBlock title="${%Additional Options}" field="hasOptions" inline="true">

		<f:entry title="${%Login User}" field="loginUser">
			<f:textbox />
		</f:entry>

		<f:entry title="${%Login Password}" field="loginPassword">
			<f:password />
		</f:entry>

		<f:entry title="${%Extra Field}" field="extraField">
			<f:textbox />
		</f:entry>
		
	</f:optionalBlock>

</j:jelly>
