	<script type="text/javascript" src="${app.rootUrl}plugin/ibm-application-security/js/util.js"></script>

	<f:entry title="${%Credentials}" field="credentials">
		<f:select />
	</f:entry>
	
	<f:entry title="${%Application}" field="application">
		<f:select />
	</f:entry>
	
	<f:entry title="${%Test Type}" field="type">
		<f:select />
	</f:entry>
	
	<f:entry title="${%Target}" field="target">
	    <f:textbox />
	</f:entry>
	
	<f:entry title="${%Name}" field="name">
	    <f:textbox />
	</f:entry>

	<f:entry field="wait">
		<f:checkbox title="${%Suspend job until security analysis completes}" onclick="waitClicked(this);" name="wait"/>
	</f:entry>

	<f:entry field="failBuild">
		<f:checkbox title="${%Fail build if:}" onclick="failBuildClicked(this);" name="failBuild"/>
	</f:entry>
	
	<f:entry field="failureConditions">
		<f:repeatableProperty field="failureConditions" minimum="0" add="${%Add Condition}"/>
	</f:entry>

</j:jelly>
